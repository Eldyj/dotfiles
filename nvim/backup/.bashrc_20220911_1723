case $- in
  *i*)
    source ~/.local/share/blesh/ble.sh --attach=none
    ;;
  *)
    return
    ;;
esac

export OSH="$HOME/.oh-my-bash"
OMB_USE_SUDO=true
plugins=(
  zoxide
  spwd
  omb
  als
  p
)

aliases=(
  lsd
  main
)

OSH_THEME='powerline'

ble-face auto_complete=fg=242
ble-face region_insert=fg=242
ble-face syntax_comment=fg=238
ble-face filename_executable=fg=117,underline
ble-face command_function=fg=117
ble-face command_alias=fg=117
ble-face command_file=fg=117
ble-face command_builtin=fg=117
ble-face command_builtin_dot=fg=117
ble-face syntax_expr=fg=110

ble-face syntax_quoted=fg=149
ble-face syntax_quotation=fg=149
ble-face syntax_varname=fg=105
ble-face varname_array=fg=105
ble-face varname_empty=fg=105
ble-face syntax_param_expansion=fg=229
ble-face syntax_error=fg=196
ble-face varname_unset=fg=249

bleopt exec_errexit_mark=
bleopt prompt_eol_mark=''

source /etc/profile.d/bash_completion.sh
source "$OSH"/oh-my-bash.sh
[[ ${BLE_VERSION-} ]] && ble-attach

export NVM_DIR="$([ -z "${XDG_CONFIG_HOME-}" ] && printf %s "${HOME}/.nvm" || printf %s "${XDG_CONFIG_HOME}/nvm")"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh" # This loads nvm

echo -ne "\033]0;$(spwd)\007"
clear
# Install Ruby Gems to ~/gems
export GEM_HOME="$HOME/gems"
export PATH="$HOME/gems/bin:$PATH"
